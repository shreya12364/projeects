package com.edu;
	import java.util.Scanner;
	public class primeNumber {
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		
		 
		if(n<=1)
		for(int i=2; n>=2;n++)
		
		if(n%2==0)
		{
			System.out.println("It is not a prime number "+n);
		}
	else
	{
		System.out.println("It is not a prime number"+n);
	}
		

	}
		
	}


